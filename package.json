{
  "name": "json-url",
  "version": "2.1.1",
  "description": "Compress JSON into compact base64 URI-friendly notation",
  "main": "dist/json-url/index.js",
  "scripts": {
    "lint": "eslint src/*",
    "compile": "rm -rf dist/; babel --plugins transform-runtime,add-module-exports -d dist/ src/",
    "prepublish": "npm test && npm run compile",
    "test": "npm run lint && npm run test:node && npm run test:karma",
    "test:node": "BABEL_ENV=test nyc mocha test/index.js --timeout 120000",
    "test:karma": "karma start --single-run",
    "perf": "babel-node test/perf.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/masotime/json-url"
  },
  "author": "Benjamin Goh <masotime@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "babel-runtime": "^6",
    "lz-string": "^1.4.4",
    "lzma": "^2.3.2",
    "msgpack5": "^3.5.1",
    "node-lzw": "^0.3.1",
    "urlsafe-base64": "^1.0.0",
    "bluebird": "^3.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-module-resolver": "^2",
    "babel-plugin-transform-runtime": "^6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6",
    "babel-preset-stage-0": "^6",
    "coveralls": "^2.13.1",
    "eslint": "^3",
    "eslint-import-resolver-babel-module": "^2",
    "eslint-plugin-import": "^2",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^2.0.4",
    "mocha": "^3.5.0",
    "nyc": "^11.2.1",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.5.6"
  }
}
